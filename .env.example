# eBay API Configuration
# Copy this file to .env and fill in your actual credentials

# Environment Settings
# Set to false for production, true for sandbox testing (default: true)
SANDBOX=true

# Sandbox Environment Credentials
CLIENT_ID_SANDBOX=your_sandbox_client_id_here
CLIENT_SECRET_SANDBOX=your_sandbox_client_secret_here

# Production Environment Credentials
CLIENT_ID_PRODUCTION=your_production_client_id_here
CLIENT_SECRET_PRODUCTION=your_production_client_secret_here

# OAuth User Token (get from eBay API Test Tool)
# https://developer.ebay.com/my/api_test_tool?index=0&api=trading&call=GetItem&variation=xml&env=production
OAUTH_TOKEN=your_oauth_token_here

# Developer Credentials (usually same for sandbox and production)
DEV_ID=your_developer_id_here
APP_ID=your_app_id_here
CERT_ID=your_certificate_id_here