# eBay API Configuration
# Copy this file to .env and fill in your actual credentials

# Sandbox Environment Credentials
CLIENT_ID_SANDBOX=
CLIENT_SECRET_SANDBOX=

# Production Environment Credentials
CLIENT_ID_PRODUCTION=
CLIENT_SECRET_PRODUCTION=

# OAuth User Token (get from eBay API Test Tool)
# https://developer.ebay.com/my/api_test_tool?index=0&api=trading&call=GetItem&variation=xml&env=production
OAUTH_TOKEN=

# Developer Credentials
DEV_ID=
APP_ID=
CERT_ID=

# Environment Settings
# Set to true for sandbox testing, false for production
SANDBOX=true